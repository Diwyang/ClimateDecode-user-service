<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
	<configuration>
		<custom-converters>
			<converter type="com.climate.decode.userservice.converter.ZonedDateTimeConverter1">
				<class-a>java.time.ZonedDateTime</class-a>
				<class-b>java.time.ZonedDateTime</class-b>
			</converter>
			<converter type="com.climate.decode.userservice.converter.AddressTypeConverter">
				<class-a>com.climate.decode.userservice.enums.AddressType</class-a>
				<class-b>com.climate.decode.userservice.enums.AddressType</class-b>
			</converter>
			<converter type="com.climate.decode.userservice.converter.MobileTypeConverter">
				<class-a>com.climate.decode.userservice.enums.MobileType</class-a>
				<class-b>com.climate.decode.userservice.enums.MobileType</class-b>
			</converter>
			<converter type="com.climate.decode.userservice.converter.RoleTypeConverter">
				<class-a>com.climate.decode.userservice.enums.RoleType</class-a>
				<class-b>com.climate.decode.userservice.enums.RoleType</class-b>
			</converter>
		</custom-converters>
	</configuration>
	<mapping>
		<class-a>com.climate.decode.userservice.dto.UserRequest</class-a>
		<class-b>com.climate.decode.userservice.entity.User</class-b>
		<field>
			<a>userId</a>
			<b>userId</b>
		</field>
		<field>
			<a>name</a>
			<b>name</b>
		</field>
		<field>
			<a>email</a>
			<b>email</b>
		</field>
		<field>
			<a>status</a>
			<b>status</b>
		</field>
		<field>
			<a>createdDateTime</a>
			<b>createdDateTime</b>
		</field>
		<field>
			<a>updatedDateTime</a>
			<b>updatedDateTime</b>
		</field>
		<field>
			<a>role</a>
			<b>role</b>
		</field>
		<field>
			<a>addresses</a>
			<b>userAddress</b>
		</field>
		<field>
			<a>mobiles</a>
			<b>userMobile</b>
		</field>
	</mapping>
	<mapping>
		<class-a>com.climate.decode.userservice.dto.UserRole</class-a>
		<class-b>com.climate.decode.userservice.entity.UserRole</class-b>
		<field>
			<a>roleType</a>
			<b>roleType</b>
		</field>
	</mapping>
	<mapping>
		<class-a>com.climate.decode.userservice.dto.UserAddress</class-a>
		<class-b>com.climate.decode.userservice.entity.UserAddress</class-b>
		<field>
			<a>addressType</a>
			<b>addressType</b>
		</field>
		<field>
			<a>address1</a>
			<b>address1</b>
		</field>
		<field>
			<a>address2</a>
			<b>address2</b>
		</field>
		<field>
			<a>city</a>
			<b>city</b>
		</field>
		<field>
			<a>state</a>
			<b>state</b>
		</field>
		<field>
			<a>country</a>
			<b>country</b>
		</field>
		<field>
			<a>pincode</a>
			<b>pincode</b>
		</field>
	</mapping>
	<mapping>
		<class-a>com.climate.decode.userservice.dto.UserMobile</class-a>
		<class-b>com.climate.decode.userservice.entity.UserMobile</class-b>
		<field>
			<a>mobileType</a>
			<b>mobileType</b>
		</field>
		<field>
			<a>mobileNo</a>
			<b>mobileNo</b>
		</field>
	</mapping>
	<mapping>
		<class-a>com.climate.decode.userservice.entity.User</class-a>
		<class-b>com.climate.decode.userservice.dto.UserResponse</class-b>
		<field>
			<a>userId</a>
			<b>userId</b>
		</field>
		<field>
			<a>name</a>
			<b>name</b>
		</field>
		<field>
			<a>email</a>
			<b>email</b>
		</field>
		<field>
			<a>status</a>
			<b>status</b>
		</field>
		<field>
			<a>createdDateTime</a>
			<b>createdDateTime</b>
		</field>
		<field>
			<a>updatedDateTime</a>
			<b>updatedDateTime</b>
		</field>
		<field>
			<a>role</a>
			<b>role</b>
		</field>
		<field>
			<a>userAddress</a>
			<b>addresses</b>
		</field>
		<field>
			<a>userMobile</a>
			<b>mobiles</b>
		</field>
	</mapping>
</mappings>
